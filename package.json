    "@myrotvorets/oav-installer": "^5.0.0",
    "@myrotvorets/opentelemetry-configurator": "^7.3.0",
    "@myrotvorets/otel-utils": "^1.1.2",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/core": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "awilix": "^9.0.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "mmdb-lib": "^2.1.0"
