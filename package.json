    "@types/chai-as-promised": "^7.1.8",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.7",
    "@types/supertest": "^6.0.2",
    "c8": "^8.0.1",
    "chai": "^4.4.0",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint-formatter-gha": "^1.4.3",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.2.0",
